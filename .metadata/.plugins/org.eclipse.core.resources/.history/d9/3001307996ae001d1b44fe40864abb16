package thread2;

public class CountThread extends Thread {
	@Override
	public void run() {
		for (int i = 500; i >0; i--) {
			System.out.println("카운트>>"+i);
			try {
				wait(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}
