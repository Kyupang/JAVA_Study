package thread2;

import java.awt.FlowLayout;

import javax.swing.JFrame;
import javax.swing.JLabel;

public class ThreadMain extends JFrame {
	JLabel count, image, day;
	public static void main(String[] args) {
		ThreadMain f = new ThreadMain();
	}	
	public ThreadMain() {
			
	}
		CountThread count = new CountThread();
		DateThread date = new DateThread();
		ImageThread image = new ImageThread();
		
		count.start();
		date.start();
		image.start();
		
		
		JFrame f = new JFrame();
		f.setSize(400, 200);
		
		FlowLayout flow = new FlowLayout();
		f.setLayout(flow);
		
		JLabel countL = new JLabel();
		JLabel dateL = new JLabel();
		JLabel imageL = new JLabel();
		
		
		f.setVisible(true);
	}
}
