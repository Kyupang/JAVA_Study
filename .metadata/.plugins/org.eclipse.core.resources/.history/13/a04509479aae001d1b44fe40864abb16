package thread2;

import javax.swing.JFrame;

public class ThreadMain {

	public static void main(String[] args) {
		CountThread count = new CountThread();
		DateThread date = new DateThread();
		ImageThread image = new ImageThread();
		
		count.start();
		date.start();
		image.start();
		
		
		JFrame f = new JFrame();
		f.setSize(400, 200);
		
		f.setVisible(true);
	}
}
