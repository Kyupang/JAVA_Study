package thread3;

import javax.swing.JFrame;

public class CarGame extends JFrame {

	public CarGame() {
		setTitle("나의 자동차 경주 게임");
		setSize(500, 300);
		setVisible(true);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	}

	public static void main(String[] args) {
		CarGame f = new CarGame();
	}

}
