package classTest4;

public class Empoloyee {
	String name;
	int age;
	String sex;
	
	static int count;
	static int avgage;
	
	public Empoloyee(String name, int age, String sex) {
		count++;
		avgage += age;
		
		this.name = name;
		this.age = age;
		this.sex = sex;
		
	}
	@Override
	public String toString() {
		return "Empoloyee [name=" + name + ", age=" + age + ", sex=" + sex + "]";
	}
}
