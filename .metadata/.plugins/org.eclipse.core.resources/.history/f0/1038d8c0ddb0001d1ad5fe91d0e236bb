package exception;

import java.io.FileWriter;

public class Exception1 {

	public static void main(String[] args) {
		FileWriter file new FileWriter("test.txt");		
		
		try {
			System.out.println("2. 문제 발생코드" + 10/0);
			
		} catch (Exception e) {
			//System.out.println("에러 발생함.");
			e.getStackTrace();
		}
		
		
	}

}
