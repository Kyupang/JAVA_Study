package thread3;

import javax.swing.ImageIcon;
import javax.swing.JLabel;

public class MyThread extends Thread{
	int x,y;
	JLabel label;
	
	public MyThread(String file, int x, int y) {
		this.x = x;
		this.y = y;
		ImageIcon icon = new ImageIcon(file);
		label = new JLabel();
		label.setIcon(icon);
		label.setBounds(x, y, 100, 100);
	}
	
	@Override
	public void run() {
		
	}
}
